programa {
  funcao inicio() {
  
  //Fácil  29 – Faça um programa que receba um número e usando laços de repetição calcule e mostre a tabuada desse número.
  inteiro n1, i
  escreva("Informe um número: ")
  leia(n1)

  para(i = 1; i <= 10; i++){
  

    escreva(n1, " x ", i , " = " ,n1  * i, "\n")

  }
  
  }
}
