programa {
  funcao inicio() {
    //EXERCÍCIO 16- Faça um algoritmo que receba um número e mostre uma mensagem caso este número seja maior que 10.
    inteiro numero

    escreva("Digite um número: ")
    leia(numero)

    se(numero > 10){
     escreva("Número maior que 10!")
    }
    se(numero <= 10){
     escreva("Número menor ou igual que 10!")
    }
  
  }
}