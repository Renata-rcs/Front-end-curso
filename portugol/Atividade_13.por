programa {
  funcao inicio() {
    //EXERCICIO 13- Crie um algoritmo capas de calcular a área de um retângulo qualquer para isso ele deve receber somente a medida de seus lados em centímetros
    real altura, largura, at

    escreva("Digite a largura do retângulo em centímetros: ")
    leia(largura)
    escreva("Digite a altura do retângulo em centímetros: ")
    leia(altura)
    limpa()

    at = largura * altura

    escreva("áreia total do retângulo é: ",at,"cm.")
  }
}
