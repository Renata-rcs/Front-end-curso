programa {
  funcao inicio() {
    //EXERCICIO 21 - Crie um algoritmo capaz de verificar se um numero é impar ou par.
  inteiro numero

  escreva("Informe um número: ")
  leia(numero)

  se(numero % 5 == 0){
    escreva("Multiplo do número 5!")
  }
  senao{
    escreva("não é multiplo do número 5!")
  }
  }
}
