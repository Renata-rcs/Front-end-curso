programa {
  funcao inicio() {
   //EXERCÍCIO 18- Faça um algoritmo que receba um número e diga se este número está no intervalo entre 100 e 200.
   inteiro numero
   escreva("Informe um número: ") 
   leia(numero)

   se(numero >= 100 e numero <= 200){
      escreva("O número informado está no intervalo entre 100 e 200!")

   }
   senao{
     escreva("Não correspondi aos critérios")
   }
  }
}
